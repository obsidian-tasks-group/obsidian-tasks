{
  "name": "obsidian-tasks",
  "version": "1.24.0",
  "description": "Task management for the Obsidian knowledge base",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "node esbuild.config.mjs production",
    "build:dev": "node esbuild.config.mjs development",
    "lint": "eslint ./src --fix && eslint ./tests --fix && tsc --noEmit --pretty && svelte-check",
    "lint:markdown": "markdownlint-cli2-fix \"**/*.md\" \"#docs-snippets\" \"#node_modules\"  \"#tests\"",
    "test": "jest --ci",
    "test:dev": "jest --watch",
    "deploy:local": "pwsh -ExecutionPolicy Unrestricted -NoProfile -File ./scripts/Test-TasksInLocalObsidian.ps1"
  },
  "keywords": [
    "obsidian",
    "obsidian-plugin",
    "obsidian-tasks",
    "task-manager",
    "task-management"
  ],
  "author": "Martin Schenck and Clare Macrae",
  "license": "MIT",
  "devDependencies": {
    "@codemirror/view": "^6.8.1",
    "@evilmartians/lefthook": "^1.1.1",
    "@tsconfig/svelte": "^3.0.0",
    "@types/jest": "^29.2.3",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "approvals": "^4.1.1",
    "async-mutex": "^0.3.1",
    "builtin-modules": "^3.3.0",
    "esbuild": "0.15.6",
    "esbuild-svelte": "^0.7.1",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "markdownlint-cli2": "^0.5.1",
    "moment": "^2.29.4",
    "obsidian": "0.14.6",
    "prettier": "^2.2.1",
    "prettier-plugin-svelte": "^2.7.0",
    "svelte": "^3.48.0",
    "svelte-check": "^2.10.3",
    "svelte-preprocess": "^4.10.1",
    "ts-jest": "^29.0.3",
    "tslib": "^2.3.1",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "boon-js": "^2.0.3",
    "chrono-node": "2.3.9",
    "eventemitter2": "^6.4.5",
    "rrule": "^2.7.1"
  }
}

{
  "name": "obsidian-tasks",
  "version": "1.8.2",
  "description": "Task management for the Obsidian knowledge base",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "node esbuild.config.mjs production",
    "build:dev": "node esbuild.config.mjs development",
    "lint": "eslint ./src --fix && eslint ./tests --fix && tsc --noEmit --pretty && svelte-check",
    "lint:markdown": "markdownlint-cli2-fix \"**/*.md\" \"#node_modules\"",
    "test": "jest --ci",
    "test:dev": "jest --watch",
    "deploy:local": "pwsh -ExecutionPolicy Unrestricted -NoProfile -File ./scripts/Test-TasksInLocalObsidian.ps1"
  },
  "keywords": [
    "obsidian",
    "obsidian-plugin",
    "obsidian-tasks",
    "task-manager",
    "task-management"
  ],
  "author": "Martin Schenck",
  "license": "MIT",
  "devDependencies": {
    "@arkweid/lefthook": "^0.7.6",
    "@codemirror/view": "^0.19.34",
    "@tsconfig/svelte": "^1.0.10",
    "@types/jest": "^26.0.22",
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "async-mutex": "^0.3.1",
    "builtin-modules": "^3.2.0",
    "esbuild": "0.13.15",
    "esbuild-svelte": "^0.6.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^27.3.1",
    "markdownlint-cli2": "^0.4.0",
    "moment": "^2.29.2",
    "obsidian": "0.14.6",
    "prettier": "^2.2.1",
    "svelte": "^3.48.0",
    "svelte-check": "^2.7.2",
    "svelte-preprocess": "^4.10.1",
    "ts-jest": "^27.0.7",
    "tslib": "^2.3.1",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "chrono-node": "2.2.6",
    "rrule": "^2.7.0"
  }
}

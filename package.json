{
    "name": "obsidian-tasks",
    "version": "7.19.1",
    "description": "Task management for the Obsidian knowledge base",
    "main": "main.js",
    "engines": {
        "node": ">=18.0.0"
    },
    "scripts": {
        "dev": "node esbuild.config.mjs",
        "build": "node esbuild.config.mjs production",
        "build:vi": "node esbuild.config.mjs production && cp ./dist/* ~/code/src/github.com/vimiomori/BigDolphin/.obsidian/plugins/obsidian-tasks-plugin-vi/",
        "build:scoot": "node esbuild.config.mjs production && cp ./dist/* ~/personal/uptoyou/.obsidian/plugins/obsidian-tasks-plugin-vi",
        "build:dev": "node esbuild.config.mjs development",
        "lint": "eslint ./src --fix && eslint ./tests --fix && tsc --noEmit --pretty && svelte-check",
        "lint:markdown": "markdownlint-cli2 --fix \"**/*.md\" ",
        "test": "jest --ci",
        "test:dev": "jest --watch",
        "test:updateSnapshot": "jest -u",
        "deploy:local": "pwsh -ExecutionPolicy Unrestricted -NoProfile -File ./scripts/Test-TasksInLocalObsidian.ps1",
        "extract-i18n": "npx i18next-parser",
        "circular-deps-text": "madge --circular --extensions ts ./src > circular-deps.txt",
        "circular-deps-image": "madge --circular --extensions ts ./src --image circular-deps.png",
        "code-docs": "typedoc \"src/**/*\""
    },
    "keywords": [
        "obsidian",
        "obsidian-plugin",
        "obsidian-tasks",
        "task-manager",
        "task-management"
    ],
    "author": "Clare Macrae and Ilyas Landikov (created by Martin Schenck)",
    "license": "MIT",
    "devDependencies": {
        "@codemirror/view": "^6.2.0",
        "@evilmartians/lefthook": "^1.7.2",
        "@testing-library/jest-dom": "^6.5.0",
        "@testing-library/svelte": "^3.2.2",
        "@tsconfig/svelte": "^3.0.0",
        "@types/jest": "^29.5.8",
        "@types/mustache": "^4.2.5",
        "@types/prettier": "^2.7.3",
        "@typescript-eslint/eslint-plugin": "^5.59.8",
        "@typescript-eslint/parser": "^5.59.8",
        "approvals": "^6.2.4",
        "async-mutex": "^0.4.0",
        "builtin-modules": "^3.3.0",
        "esbuild": "0.15.6",
        "esbuild-sass-plugin": "^3.2.0",
        "esbuild-svelte": "^0.8.0",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-svelte3": "^4.0.0",
        "i18next-parser": "^9.1.0",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.5.0",
        "madge": "^8.0.0",
        "markdownlint-cli2": "^0.13.0",
        "moment": "^2.29.4",
        "obsidian": "^1.4.0",
        "obsidian-typings": "^2.11.1",
        "prettier": "^2.8.8",
        "prettier-plugin-svelte": "^2.10.1",
        "svelte": "^3.59.1",
        "svelte-check": "^3.8.4",
        "svelte-eslint-parser": "^0.43.0",
        "svelte-jester": "^2.3.2",
        "svelte-preprocess": "^5.0.4",
        "ts-jest": "^29.2.5",
        "tslib": "^2.5.2",
        "typedoc": "^0.27.3",
        "typedoc-plugin-mdn-links": "^4.0.4",
        "typedoc-umlclass": "^0.10.1",
        "typescript": "5.0.4"
    },
    "dependencies": {
        "@floating-ui/dom": "^1.6.12",
        "boon-js": "^2.0.5",
        "chrono-node": "2.3.9",
        "eventemitter2": "^6.4.5",
        "flatpickr": "^4.6.13",
        "i18next": "^24.2.1",
        "mustache": "^4.2.0",
        "mustache-validator": "^0.2.0",
        "rrule": "^2.7.2"
    }
}
